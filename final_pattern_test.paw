type Option = enum {
    Some(i32),
    None(),
}

fn main() -> i32 {
    let val1: Option = Option::Some(42);
    let val2: Option = Option::None();
    
    // match表达式
    let r1: i32 = val1 is {
        Some(x) => x * 2,
        None() => 0,
    };
    
    let r2: i32 = val2 is {
        Some(x) => x,
        None() => 99,
    };
    
    return r1 + r2;  // 84 + 99 = 183
}
