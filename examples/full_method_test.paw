// 完整的方法系统测试

type Counter = struct {
    value: i32,
    
    fn new(init: i32) -> Counter {
        return Counter { value: init };
    }
    
    fn get(self) -> i32 {
        return self.value;
    }
}

fn main() -> i32 {
    // 测试关联函数
    let c: Counter = Counter::new(42);
    
    // 测试实例方法
    let v: i32 = c.get();
    
    return v;
}

