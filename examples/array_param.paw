// 测试数组作为参数传递

fn get_first(arr: [i32; 3]) -> i32 {
    return arr[0];
}

fn sum_two(arr: [i32; 5]) -> i32 {
    return arr[0] + arr[1];
}

fn main() -> i32 {
    let numbers: [i32; 5] = [10, 20, 30, 40, 50];
    let small: [i32; 3] = [1, 2, 3];
    
    let first: i32 = get_first(small);
    let sum: i32 = sum_two(numbers);
    
    return first + sum;
}

