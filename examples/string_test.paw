// 测试纯 Paw 字符串操作编译成什么

fn count_digits(s: string) -> i32 {
    let len: i32 = 10;  // 简化：假设长度
    let mut count: i32 = 0;
    let mut i: i32 = 0;
    
    loop i in 0..len {
        let c: i32 = s[i] as i32;
        if c >= 48 && c <= 57 {  // '0' to '9'
            count += 1;
        }
    }
    
    return count;
}

fn main() -> i32 {
    let test: string = "abc123xyz";
    return count_digits(test);
}

