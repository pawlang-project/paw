// é€’å½’ Fibonacciï¼ˆæœªä¼˜åŒ–ä¼šå¾ˆæ…¢ï¼‰
fn fibonacci(n: i32) -> i32 {
    return if n <= 1 {
        n
    } else {
        fibonacci(n - 1) + fibonacci(n - 2)
    };
}

// è¿­ä»£ Fibonacciï¼ˆå¯¹æ¯”ï¼‰
fn fibonacci_iterative(n: i32) -> i32 {
    if n <= 1 {
        return n;
    }
    
    let mut a = 0;
    let mut b = 1;
    let mut i = 2;
    
    loop i <= n {
        let tmp = a + b;
        a = b;
        b = tmp;
        i = i + 1;
    }
    
    return b;
}

