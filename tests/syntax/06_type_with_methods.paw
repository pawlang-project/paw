// 测试 6: type 定义带方法的 struct

type Calculator = struct {
    value: i32
    
    fn get(self) -> i32 {
        self.value
    }
    
    fn add(self, n: i32) -> i32 {
        self.value + n
    }
}

type Point = struct {
    x: i32
    y: i32
    
    fn sum(self) -> i32 {
        self.x + self.y
    }
}

fn main() -> i32 {
    let calc = Calculator { value: 100 };
    let r1 = calc.get();
    let r2 = calc.add(50);
    
    let p = Point { x: 3, y: 4 };
    let r3 = p.sum();
    
    r1 + r2 + r3
}


