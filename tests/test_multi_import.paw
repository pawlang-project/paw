// 🆕 多项导入语法
import math.{add, multiply, Vec2, Point};

fn main() -> i32 {
    // 使用导入的函数
    let sum = add(10, 20);
    let product = multiply(5, 6);
    
    // 使用导入的类型
    let v = Vec2 { x: 1, y: 2 };
    let p = Point { x: 3, y: 4 };
    
    return sum + product + v.x + p.x;
}

